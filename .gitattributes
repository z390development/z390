* text=auto
*.sh text eol=lf
bash/** text eol=lf
*.png filter=lfs diff=lfs merge=lfs -text
*.bmp  -diff -merge -text
*.doc  -diff -merge -text
*.docx -diff -merge -text
*.gif  -diff -merge -text
*.ico  -diff -merge -text
*.jpg  -diff -merge -text
*.pdf  -diff -merge -text
*.pps  -diff -merge -text
*.ppt  -diff -merge -text
*.rtf  -diff -merge -text
*.zip  -diff -merge -text
