REM ZPARERR.BAT CREATE ZPARERR.RPT FROM %1 SOURCES USING ZPARERR.MLC
COPY %1.ERR zpar\ZPARERR.DAT 
SET SYSUT1=zpar\ZPARERR.DAT
SET SYSUT2=zpar\ZPARERR.TMP
ERASE zpar\ZPARERR.TMP
ERASE zpar\ZPARERR.RPT
CALL MAC zpar\ZPARERR SYSPARM(%1.ERR) %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR RUNNING zpar\ZPARERR.MLC
SORT < zpar\ZPARERR.TMP > zpar\ZPARERR.RPT
dir zpar\ZPARERR.*
