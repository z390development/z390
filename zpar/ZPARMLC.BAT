REM ZPARMLC.BAT CREATE ZPARMLC.RPT FROM %1 SOURCES USING ZPARMLC.MLC
COPY %1 zpar\ZPARMLC.DAT 
SET SYSUT1=zpar\ZPARMLC.DAT
SET SYSUT2=zpar\ZPARMLC.TM1
SET SYSUT3=zpar\ZPARMLC.TM2
ERASE zpar\ZPARMLC.TM1
ERASE zpar\ZPARMLC.TM2
ERASE zpar\ZPARMLC.RP1
ERASE zpar\ZPARMLC.RP2
ERASE zpar\ZPARMLC.RPT
CALL MAC zpar\ZPARMLC TIME(300) %2 %3 %4 %5 %6 %7 %8 %9 
IF ERRORLEVEL 1 PAUSE ERROR RUNNING zpar\ZPARMLC.MLC
SORT /R < zpar\ZPARMLC.TM1 > zpar\ZPARMLC.RP1
SORT    < zpar\ZPARMLC.TM2 > zpar\ZPARMLC.RP2
COPY    zpar\ZPARMLC.RP1+zpar\ZPARMLC.RP2 zpar\ZPARMLC.RPT
dir zpar\ZPARMLC.*
