         AGO   .RES080
.RES00A  AIF   ('&SMC' EQ '' AND '&RET' EQ '' AND '&UCB' EQ '').RES01A
         AIF   ('&SMC' EQ '' AND '&RET' EQ '').RES02A
         AIF   ('&RET' EQ '' AND '&UCB' EQ '').RES03A
         AIF   ('&SMC' EQ '' AND '&UCB' EQ '').RES04A
         AIF   ('&UCB' EQ '').RES05A
         AIF   ('&SMC' EQ '').RES06A
         AIF   ('&RET' EQ '').RES07A
&NAME    ENQ   &PARAM,SMC=&SMC,RET=&RET,UCB=&UCB,MF=&MF
         AGO   .RES080
.RES01A  ANOP
