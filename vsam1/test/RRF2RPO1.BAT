REM LOAD/UNLOAD RRF2 RCDS 1,3,5 USING DEFAULT RF RRN,RCD FORMAT
REM RUN REPRO TO LOAD RRF2.VES FROM RRF2.TF1
SET INFILE=vsam\test\RRF2.TF1
SET OUTFILE=vsam\test\TESTCAT.RRF2
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO LOAD
REM SAVE REPRO LOAD STATS
COPY REPRO.LOG vsam\test\RRF2RPO1.RLG  
COPY REPRO.TRE vsam\test\RRF2RPO1.RLE
REM RUN REPRO TO UNLOAD FROM RRF2.VES TO RRF2.TF2
SET INFILE=vsam\test\TESTCAT.RRF2
SET OUTFILE=vsam\test\RRF2.TF2
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO UNLOAD
REM SAVE REPRO UNLOAD STATS
COPY REPRO.LOG vsam\test\RRF2RPO1.RUG  
COPY REPRO.TRE vsam\test\RRF2RPO1.RUE
REM COMPARE INPUT AND OUTPUT QSAM FILES
CALL rt\COMP1 vsam\test\RRF2.TF1 vsam\test\RRF2.TF2 vsam\test\RRF2RPO1.DIF
IF ERRORLEVEL 1 PAUSE REPRO LOAD/UNLOAD QSAM FILES DONT MATCH

