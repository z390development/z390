REM LOAD/UNLOAD ESF4 USING RECFM=FT
SET INFILE=vsam\test\ESF4.TQ1[RECFM=FT]
SET OUTFILE=vsam\test\TESTCAT.ESF4
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO LOAD
REM SAVE REPRO LOAD STATS
COPY REPRO.LOG vsam\test\ESF4RPQ1.RLG  
COPY REPRO.TRE vsam\test\ESF4RPQ1.RLE
REM RUN REPRO TO UNLOAD FROM ESF4.VES TO ESF4.TF2
SET INFILE=vsam\test\TESTCAT.ESF4
SET OUTFILE=vsam\test\ESF4.TQ2[RECFM=FT]
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO UNLOAD
REM SAVE REPRO UNLOAD STATS
COPY REPRO.LOG vsam\test\ESF4RPQ1.RUG  
COPY REPRO.TRE vsam\test\ESF4RPQ1.RUE
REM COMPARE INPUT AND OUTPUT QSAM FILES
CALL rt\COMP1 vsam\test\ESF4.TQ1 vsam\test\ESF4.TQ2 vsam\test\ESF4RPQ1.DIF
IF ERRORLEVEL 1 PAUSE REPRO LOAD/UNLOAD QSAM FILES DONT MATCH
