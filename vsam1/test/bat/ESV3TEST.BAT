@echo off
REM ESF3TEST TEST ESDS VAR LENGTH VSAM FILE SEQ AND RANDOM RBA/XRBA
rem %0 in z390\vsam1\test\bat; switch to z390
pushd %~dps0..\..\..

SET VSAMPATH=vsam1\test
SET ZVSAM=ZVSAM(1)
SET TRACE=%
IF /I "%1" == "traceall" (
    SET TRACE=TRACEALL
    shift /1
)

SET RC=0

REM RUN ESV3TEST TO CREATE AND TEST ESDS WITH 3 80 BYTE RCDS
SET VFILE=%VSAMPATH%\TESTCAT.ESV3
if exist %VSAMPATH%\ESV3.VES erase %VSAMPATH%\ESV3.VES
CALL bat\ASMLG %VSAMPATH%\ESV3TEST %1 %2 %3 %4 %5 %ZVSAM% %TRACE%
SET RC=%ERRORLEVEL%
IF %RC% NEQ 0 (
    SET ERRMSG=ERROR IN ESV3TEST
	GOTO errmsg
)

:return
popd
exit /b %RC%
:errmsg
echo ERROR: %ERRMSG%
SET RC=8
GOTO return
