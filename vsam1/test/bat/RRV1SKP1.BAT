@echo off
REM RRV1SKP1 TEST VRRDS VAR LENGTH VSAM FILE SKP SEQ ACCESS USING GENCB
REM FIRST RUN RRV1TEST TO CREATE THE RRDS
rem %0 in z390\vsam1\test\bat; switch to z390
pushd %~dps0..\..\..

SET VSAMPATH=vsam1\test
SET ZVSAM=ZVSAM(1)
SET TRACE=%
IF /I "%1" == "traceall" (
    SET TRACE=TRACEALL
    shift /1
)

SET RC=0

REM RUN RRV1SKP1 TO TEST ESDS WITH 3 80 BYTE RCDS
SET VFILE=%VSAMPATH%\TESTCAT.RRV1
CALL bat\ASMLG %VSAMPATH%\RRV1SKP1 %1 %2 %3 %4 %5 %ZVSAM% %TRACE%
SET RC=%ERRORLEVEL%
IF %RC% NEQ 0 (
    SET ERRMSG=ERROR IN RRV1SKP1
	GOTO errmsg
)

:return
popd
exit /b %RC%
:errmsg
echo ERROR: %ERRMSG%
SET RC=8
GOTO return
