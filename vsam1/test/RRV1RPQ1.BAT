REM LOAD/UNLOAD RRV1 USING RECFM=V QSAM FILE    
REM NOTE RRV1.TF1 QSAM INPUT FILE IS CREATED BY RRV1TEST.MLC
REM RUN REPRO TO LOAD RRV1.VES FROM RRV1.TF1
SET INFILE=vsam\test\RRV1.TQ1[RECFM=V]
SET OUTFILE=vsam\test\TESTCAT.RRV1
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO LOAD
REM SAVE REPRO LOAD STATS
COPY REPRO.LOG vsam\test\RRV1RPQ1.RLG  
COPY REPRO.TRE vsam\test\RRV1RPQ1.RLE
REM RUN REPRO TO UNLOAD FROM RRV1.VES TO RRV1.TF2
SET INFILE=vsam\test\TESTCAT.RRV1
SET OUTFILE=vsam\test\RRV1.TQ2[RECFM=V]
CALL EXEC REPRO %1 %2 %3 %4 %5 %6 %7 %8 %9
IF ERRORLEVEL 1 PAUSE ERROR IN REPRO UNLOAD
REM SAVE REPRO UNLOAD STATS
COPY REPRO.LOG vsam\test\RRV1RPQ1.RUG  
COPY REPRO.TRE vsam\test\RRV1RPQ1.RUE
REM COMPARE INPUT AND OUTPUT QSAM FILES
CALL rt\COMP1 vsam\test\RRV1.TQ1 vsam\test\RRV1.TQ2 vsam\test\RRV1RPQ1.DIF
IF ERRORLEVEL 1 PAUSE REPRO LOAD/UNLOAD QSAM FILES DONT MATCH

