* switch to amode 31
*
t1
* ########################################
* ### after SAM31: key 8, CC 3, amode 31
psw
  psw
psw+
  psw+
  v psw.key   = x'8'
  v psw.cc    = x'3'
  v psw.amode = x'1'
* ########################################
*
* switch to amode 64
*
t1
* ########################################
* ### after SAM64: key 8, CC 3, amode 64
psw
  psw
psw+
  psw+
  v psw.key   = x'8'
  v psw.cc    = x'3'
  v psw.amode = x'3'
